{
  "name": "kaleemullahdev-monorepo",
  "version": "0.1.0",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "dev": "turbo run dev",
    "build": "turbo run build",
    "start": "turbo run start",
    "lint": "turbo run lint",
    "type-check": "turbo run type-check",
    "clean": "turbo run clean",
    "studio:dev": "turbo run dev --filter=studio",
    "web:dev": "turbo run dev --filter=web"
  },
  "devDependencies": {
    "turbo": "^2.5.6",
    "prettier": "^3.5.3"
  },
  "packageManager": "npm@10.8.2"
}